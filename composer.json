{
    "name": "diego-ninja/larasoul",
    "description": "A Verisoul integration for Laravel 10+",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ninja\\Larasoul\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Diego Rin",
            "email": "yosoy@diego.ninja"
        }
    ],
    "extra": {
      "laravel": {
        "providers": [
          "Ninja\\Larasoul\\Providers\\LarasoulServiceProvider"
        ],
        "aliases": {
          "Verisoul": "Ninja\\Larasoul\\Facades\\Verisoul"
        }
      }
    },
    "minimum-stability": "stable",
    "require": {
      "php": "^8.2",
      "laravel/framework": "^12.0",
      "laravel/tinker": "^2.10.1",
      "nesbot/carbon": "^3.10",
        "dshafik/bag": "^2.5"
    },
    "require-dev": {
      "barryvdh/laravel-ide-helper": "^3.1",
      "fakerphp/faker": "^1.24",
      "larastan/larastan": "^3.0",
      "laravel/pint": "^1.18",
      "mockery/mockery": "^1.4.4",
      "orchestra/testbench": "^10.4",
      "pestphp/pest": "^3",
      "phpstan/phpstan": "^2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
